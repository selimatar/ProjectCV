@model ProjectCV.Models.Entity.TblAboutPerson
@{
    ViewBag.Title = "TraitDatas";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

@using (Html.BeginForm("UpdateTrait", "About", FormMethod.Post))
{
    <div class="form-group">
        @Html.LabelFor(m => m.id)
        @Html.TextBoxFor(m => m.id, new { @class = "form-control" })
        <br />
        @Html.LabelFor(m => m.trait)
        @Html.TextBoxFor(m => m.trait, new { @class = "form-control" })
        <br />
        <button class="btn btn-primary">Update</button>
    </div>
}
